
# Variables d'environnement pour JJ Mécanique

# Google Maps API (pour calcul précis des distances)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Calendar API (pour gestion des rendez-vous)
REACT_APP_GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here

# Square Payment API (pour paiements en ligne)
REACT_APP_SQUARE_APPLICATION_ID=your_square_app_id_here
REACT_APP_SQUARE_LOCATION_ID=your_square_location_id_here

# Mailchimp API (pour infolettre)
REACT_APP_MAILCHIMP_API_KEY=your_mailchimp_api_key_here
REACT_APP_MAILCHIMP_AUDIENCE_ID=your_mailchimp_audience_id_here

# Configuration Email (pour envoi des devis)
REACT_APP_EMAIL_SERVICE_API_KEY=your_email_service_api_key_here

# Base URL de l'API
REACT_APP_API_BASE_URL=https://api.jjmecanique.ca

# Mode développement
NODE_ENV=development
